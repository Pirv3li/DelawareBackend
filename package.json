{
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "argon2": "^0.31.2",
    "bcrypt": "^5.1.1",
    "config": "^3.3.9",
    "joi": "^17.11.0",
